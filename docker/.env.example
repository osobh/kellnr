# Kellnr Docker Compose Environment Variables
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control!

# PostgreSQL Configuration
POSTGRES_USER=kellnr
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD
POSTGRES_DB=kellnr

# Kellnr Admin Credentials (only used on first startup)
# After first start, change password via web UI
KELLNR_ADMIN_PWD=CHANGE_ME_ADMIN_PASSWORD
KELLNR_ADMIN_TOKEN=CHANGE_ME_GENERATE_32_CHAR_TOKEN
